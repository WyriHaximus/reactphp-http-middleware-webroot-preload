{
    "name": "wyrihaximus/react-http-middleware-webroot-preload",
    "description": "react/http middleware static webroot serving",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ancarda/psr7-string-stream": "^1.3",
        "league/mime-type-detection": "^1.9",
        "psr/http-message": "^1",
        "psr/log": "^1.1",
        "react/cache": "^1",
        "react/http": "^1",
        "react/promise": "^2.8",
        "scriptfusion/byte-formatter": "^3.2",
        "thecodingmachine/safe": "^1.3",
        "wyrihaximus/constants": "^1.5"
    },
    "require-dev": {
        "brunty/cigar": "^1.12",
        "wyrihaximus/async-test-utilities": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\Http\\Middleware\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\React\\Tests\\Http\\Middleware\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "icanhazstring/composer-unused": true,
            "infection/extension-installer": true
        },
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
